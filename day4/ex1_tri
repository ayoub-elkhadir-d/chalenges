#include <stdio.h>


int main() {
    int tab[] = {64, 34, 25, 12, 22, 11, 90};
    int taille = sizeof(tab) / sizeof(tab[0]);

  
    int temp;
    for (int i = 0; i < taille - 1; i++) {
        for (int j = 0; j < taille - i - 1; j++) {
            if (tab[j] > tab[j + 1]) {
                
                temp = tab[j];
                tab[j] = tab[j + 1];
                tab[j + 1] = temp;
            }
        }
    }
    
    for (int i = 0; i < taille; i++) {
        printf("%d ", tab[i]);
    }
    printf("\n");

    return 0;
}
