#include <stdio.h>

void fib(int n) {
    int a1 = 0, a2 = 1, next;

    
    for (int i = 1; i <= n; i++) {
        printf("%d ", a1);
        
        next = a1 + a2;
        a1 = a2;
        a2 = next;
    }
   
}

int main() {
    int n;
    printf("Entrez un num;");
    scanf("%d", &n);
    fib(n);
    return 0;
}
