// Online C compiler to run C program online
#include <stdio.h>
void main() {
    int size;
    int min ;
printf("enter size ");
scanf("%d",&size);

int num[size];
for( int a =0;a < size;a++){
    int value =0;
   printf("enter num %d : ",a+1);
   scanf("%d",&value);
   num[a]=value;
}
 
  for(int y =0;y < size;y++){
if(num[y] < num[y+1])
      min = num[y];
  } 
  printf(" THE min : %d",min);
  
}
