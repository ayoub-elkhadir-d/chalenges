#include <stdio.h>

void main() {
    int n, i, j,e,en;
    int is_true=0;

    
    printf("enter lenth of tablou : ");
    scanf("%d", &n);
 printf("enter element to replaced  : ");
    scanf("%d", &e);
    
    printf("enter new element  : ");
    scanf("%d", &en);
    int arr[n];
    int s = sizeof(arr)/sizeof(arr[0]);
   
    
    for(i = 0; i < n; i++) {
        printf("enter num %d : ", i + 1);
        scanf("%d", &arr[i]);
    }
    
    for(int z = 0;z<= n;z++){
    	if(arr[z]==e){
    		arr[z]=en;
    		}
    	
    	}
    
    
for(int r =0;r<n;r++){
    printf(" %d,",arr[r]);
    
    
    
}

    
   
}
