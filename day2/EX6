// Online C compiler to run C program online
#include <stdio.h>
void main() {
    int size;
    int f ;
printf("enter size ");
scanf("%d",&size);
printf("enter facteur:");
scanf("%d",&f);
int num[size];
for( int a =0;a < size;a++){
    int value =0;
   printf("enter num %d : ",a+1);
   scanf("%d",&value);
   num[a]=value*f;
}
 
  for(int y =0;y < size;y++){
 printf("\n %d",num[y]);
  } 
 
  
}
