#include <stdio.h>

int main() {
    int base, ex;
    float resultat = 1;

    printf("Entrez la base: ");
    scanf("%d", &base);

    printf("Entrez l'exposant: ");
    scanf("%d", &ex);

    for(int i = 1; i <= ex; i++) {
        resultat *= base;
    }

    printf("%d ^ %d = %.2f\n", base, ex, resultat);
    return 0;
}
